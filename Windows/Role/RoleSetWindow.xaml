<Window x:Class="StudyTracker.Windows.Role.RoleSetWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:StudyTracker.Windows.Role"
        mc:Ignorable="d"
        Title="RoleSetWindow" Height="200" Width="300"
        ResizeMode="NoResize">
    <Grid>
        <TextBlock Margin="83,100,180,114">Роль</TextBlock>
        <TextBlock Margin="83,53,135,161">Пользователь</TextBlock>
        <ComboBox Name="User" HorizontalAlignment="Center" Margin="0,45,0,0" VerticalAlignment="Top" Width="150" Height="30"/>
        <ComboBox Name="Role" HorizontalAlignment="Center" Margin="0,80,0,0" VerticalAlignment="Top" Width="150" Height="30"/>
        <Button Content="Выдать роль" Click="SetRoleButton" HorizontalAlignment="Center" Margin="0,120,0,0" VerticalAlignment="Top" Height="25" Width="150"/>
    </Grid>
</Window>
